{
  "name": "@zooroyal/vertical-components",
  "version": "0.0.1",
  "pritave": true,
  "description": "Vertical Web Components",
  "keywords": [
    "vertical",
    "webcomponents"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:mrmoree/VerticalComponents2.git"
  },
  "bugs": {
    "url": "https://github.com/ZooRoyal/VerticalComponents2/issues"
  },
  "homepage": "https://github.com/ZooRoyal/VerticalComponents2#README",
  "author": "Oliver Kierepka <o.kierepka@zooroyal.de> (https://zooroyal.de)",
  "license": "MIT",
  "devDependencies": {
    "@zooroyal/vert-dev-toolbox": "^0.0.4"
  },
  "scripts": {
    "test": "jest",
    "del-dist": "lerna exec --parallel -- del-cli dist",
    "prebuild": "yarn del-dist",
    "build": "lerna exec -- babel src -d dist --ignore test.js",
    "start": "node packages/catalog/dist/catalog"
  },
  "npmClient": "yarn",
  "private": true,
  "useWorkspaces": true,
  "workspaces": {
    "packages": [
      "elements/*",
      "themes/*"
    ],
    "nohoist": []
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true
  }
}
