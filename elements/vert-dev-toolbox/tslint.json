{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "eofline": false,
    "no-trailing-whitespace": false,
    "comment-format": false,
    "quotemark": false,
    "no-console": false,
    "one-line": false,
    "no-consecutive-blank-lines": false,
    "curly": false
  },
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.{j,t}s",
      "**/node_modules/**/*.{j,t}s"
    ]
  },
  "rulesDirectory": []
}
